/* pagesUser/login/login.wxss */
.container {
  padding: 50rpx;
  background: #fff;
  min-height: 100%;
  box-sizing: border-box;
}
.logo {
  display: block;
  width: 248rpx;
  margin: 90rpx auto 40rpx;
}
.name {
  line-height: 48rpx;
  margin-bottom: 30rpx;
  text-align: left;
  font-size: 34rpx;
  font-weight: 600;
  color: var(--blackColor);
}
.txt {
  line-height: 40rpx;
  margin-bottom: 80rpx;
  text-align: left;
  font-size:28rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(102,102,102,1);
}
.mp-btn {
background:rgba(46,145,9,1);
border-radius:10rpx;
  margin: 0 auto;
  font-size:34rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(255,255,255,1);
}
.mp-btn::after {border-radius: 88rpx;}